<h2>Authenticated with sudo access!</h2>

<button id="returnHome" onclick="location.href='<%= url %>' + '/token/' + '<%= token %>' + '/add'">Add New
    Address</button>
<br>
<button id="returnHome" onclick="window.open('<%= url %>' + '/token/' + '<%= token %>' + '/uploadcsv')">Batch Upload
    (CSV)</button>



<h4>Email</h4>
<input id="inputEmail" value="">
<button id="buttonEmail"
    onclick="location.href='<%= url %>' + '/token/' + '<%= token %>' + '/email/'+encodeURIComponent(document.getElementById('inputEmail').value)">Search
    by
    Email</button>

<script>
    var input = document.getElementById("inputEmail");
    input.addEventListener("keypress", function (event) {
        if (event.key === "Enter") {
            event.preventDefault();
            document.getElementById("buttonEmail").click();
        }
    });
</script>

